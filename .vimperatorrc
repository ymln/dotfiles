cmap <C-b> <Left>
cmap <C-f> <Right>
cmap <A-b> <C-Left>
cmap <A-f> <C-Right>
nmap , gT
nmap . gt
nmap J gt
nmap K gT
imap <C-b> <Left>
imap <C-f> <Right>
imap <A-b> <C-Left>
imap <A-f> <C-Right>
map e :bdelete!<Space>
map x :open<Space>d<Space>
map <lt> :tabmove!<Space>-1<Return>
map > :tabmove!<Space>+1<Return>
set defsearch=ddg
source! ~/.vimperatorrc.local
set editor=EDITOR